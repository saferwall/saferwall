{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "User",
    "description": "This schema is representing an action over a file",
    "type": "object",
    "properties": {
      "type": {
        "description": "Type of action to perform",
        "type": "string",
        "pattern": "^(rescan|download|like|unlike)$"
      }
    },
    "required": ["type"],
    "additionalProperties": false
  }
  