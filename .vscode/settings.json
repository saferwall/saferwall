{
    "search.exclude": {
        "**/node_modules": true,
    },
    "files.watcherExclude": {
        "**/node_modules": true,
    },
    "git.ignoreLimitWarning": true,
    "files.exclude": {
        "_work/": true,
        "_diag/": true,
        ".vault/": true,
        "externals/": true,
        ".credentials*": true,
        ".path": true,
        ".runner": true,
        "coverage*": true,
        "tags.lock*": true,
        "topic*.dat": true,
        "*meta.dat": true,
        "nsqd.dat": true,
        "*/**/main": true,
        "*/**/__debug_bin": true,
    },
    "editor.rulers": [80,120],
    "editor.renderWhitespace": "all",
}
